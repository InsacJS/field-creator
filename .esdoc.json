{
  "source": "./lib",
  "destination": "./docs/",
  "includes": ["\\.js$"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "undocumentIdentifier": { "enable": false },
        "unexportedIdentifier": { "enable": true },
        "lint": { "enable": true },
        "coverage": { "enable": true },
        "typeInference": { "enable": true },
        "brand": {
          "title": "Field Creator",
          "description": "Simplifica la definici√≥n de atributos para crear un modelo Sequelize.",
          "repository": "https://github.com/insacjs/field-creator"
        }
      }
    }
  ]
}
